         "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<typeAliases>
		<typeAlias type="com.midas.websolution.user.vo.UserVO" alias="UserVO"/>	
		<typeAlias type="com.midas.websolution.menu.vo.MenuVO" alias="MenuVO"/>	
		<typeAlias type="com.midas.websolution.menu.vo.FoodVO" alias="FoodVO"/>
		<typeAlias type="com.midas.websolution.menu.vo.FoodSetVO" alias="FoodSetVO"/>	
