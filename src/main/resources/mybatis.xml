		<typeAlias type="com.midas.websolution.menu.vo.MenuResultVO" alias="MenuResultVO"/>	
	</typeAliases>

    <mappers>
    	<!-- Paging 관련 쿼리들이 있는 매퍼 -->
        <mapper resource="commonDaoSql.xml"/>
